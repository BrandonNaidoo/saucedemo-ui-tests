{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "es6"],
    "module": "esnext",
    "moduleResolution": "node",
    "types": ["cypress"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": false
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"]
}
